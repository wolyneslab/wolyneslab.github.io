 date-added = {2020-07-22T21:09:45GMT},
 date-modified = {2020-07-22T21:35:28GMT},
 doi = {10.1073/pnas.95.19.11037},
 file = {{D42962DA-F7B1-43AA-BFD1-9A1601AD7EBE.pdf:/Users/weilu/Dropbox/Papers Library/Files/D4/D42962DA-F7B1-43AA-BFD1-9A1601AD7EBE.pdf:application/pdf}},
 journal = {Proceedings of the National Academy of Sciences},
 language = {English},
 local-url = {file://localhost/Users/weilu/Dropbox/Papers%20Library/Files/D4/D42962DA-F7B1-43AA-BFD1-9A1601AD7EBE.pdf},
 number = {19},
 pages = {11037--11038},
 pmid = {9736683},
 publisher = {National Academy of Sciences},
 rating = {0},
 title = {{Chemical physics of protein folding}},
 uri = {\url{papers3://publication/doi/10.1073/pnas.95.19.11037}},
 url = {http://www.pnas.org/cgi/doi/10.1073/pnas.95.19.11037},
