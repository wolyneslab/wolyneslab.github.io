 date-modified = {2020-07-22T21:09:55GMT},
 journal = {arXiv},
 language = {English},
 pages = {arXiv:0902.4078},
 rating = {0},
 title = {{Wavenumber dependence of the structural relaxation time in the crossover regime of supercooled liquid dynamics}},
