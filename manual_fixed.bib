 doi = {10.1073/pnas.94.12.6170},
 file = {{0011D519-BA72-49D4-8D02-21E6CDDD3814.pdf:/Users/weilu/Dropbox/Papers Library/Files/00/0011D519-BA72-49D4-8D02-21E6CDDD3814.pdf:application/pdf}},
