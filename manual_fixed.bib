 doi = {10.1021/jp409502k},
 file = {{FF6550C7-0882-4D5A-AFCF-FB9745B89D14.pdf:/Users/weilu/Dropbox/Papers Library/Files/FF/FF6550C7-0882-4D5A-AFCF-FB9745B89D14.pdf:application/pdf}},
 journal = {The Journal of Physical Chemistry B},
 language = {English},
 local-url = {file://localhost/Users/weilu/Dropbox/Papers%20Library/Files/FF/FF6550C7-0882-4D5A-AFCF-FB9745B89D14.pdf},
 number = {48},
 pages = {15204--15219},
 publisher = {American Chemical Society},
