 language = {English},
 number = {3},
 pages = {031509},
 publisher = {American Physical Society},
 rating = {0},
 title = {{Bridging the gap between the mode coupling and the random first order transition theories of structural relaxation in liquids}},
 uri = {\url{papers3://publication/doi/10.1103/PhysRevE.72.031509}},
 url = {https://link.aps.org/doi/10.1103/PhysRevE.72.031509},
 volume = {72},
